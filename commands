$("#submit").click(function(){
  var username = $("#username").val()
  var password = $("#password").val()

  var formBody = [];
  var encodedKey = encodeURIComponent("username")
  var encodedValue = encodeURIComponent(username);
  formBody.push(encodedKey + "=" + encodedValue);

  encodedKey = encodeURIComponent("password")
  encodedValue = encodeURIComponent(password);
  formBody.push(encodedKey + "=" + encodedValue);

  formBody = formBody.join("&");

  console.log(formBody.toString())

  fetch("/token", {
    method: "POST",
    headers: {
      "Content-Type": "aplication/application/x-www-form-urlencoded"
    },
    body: formBody
  })
  .then(res => {
    console.log('aaaaaaaaaa')
    console.log(res)
    console.log(res.toString)
  })
})
